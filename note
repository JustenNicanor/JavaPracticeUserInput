for (int count = 0; count < arr.length; count++) {
                int minIndex = -1;
                int minValue = Integer.MAX_VALUE;

                for (int i = 0; i < arr.length; i++) {
                    if (arr[i] < minValue) {
                        minValue = arr[i];
                        minIndex = i;
                    }
                }
                arr[minIndex] = Integer.MAX_VALUE;
                System.out.print(minValue + ", ");

            }